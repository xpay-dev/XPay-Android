<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.transaction.receipt.ReceiptFragment">

    <data>
        <variable
            name="txns"
            type="com.xpayworld.payment.network.TransactionResponse" />
        <variable
            name="response"
            type="com.xpayworld.payment.network.PosWsResponse" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imgBackground"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/btnDone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView12">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clReceipt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/print_preview_body"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/lblStatus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Transaction Status"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvAuthNumber" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.5" />

                <TextView
                    android:id="@+id/lblMerchantName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="ABC MERCHANT"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_name"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/lblMerchantAddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Makati , Philippines"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/lblMerchantName"
                    app:layout_constraintStart_toStartOf="@+id/lblMerchantName"
                    app:layout_constraintTop_toBottomOf="@+id/lblMerchantName" />

                <TextView
                    android:id="@+id/lblDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:gravity="center_horizontal"
                    android:text="Date"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lblMerchantAddress" />

                <TextView
                    android:id="@+id/dividerDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblDate"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblDate" />

                <TextView
                    android:id="@+id/dividerTerminalId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblTerminalId"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblTerminalId" />

                <TextView
                    android:id="@+id/dividerMerchantId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblMerchantId"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblMerchantId" />

                <TextView
                    android:id="@+id/dividerBatchNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblBatchNumber"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblBatchNumber" />

                <TextView
                    android:id="@+id/dividerInvoice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblInvoiceNumber"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblInvoiceNumber" />

                <TextView
                    android:id="@+id/dividerCardNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblCardNumber"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblCardNumber" />

                <TextView
                    android:id="@+id/dividerCardType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblCardType"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblCardType" />

                <TextView
                    android:id="@+id/dividerAuthNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblAuthNumber"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblAuthNumber" />

                <TextView
                    android:id="@+id/dividerStatus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" : "
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/lblStatus"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toTopOf="@+id/lblStatus" />

                <TextView
                    android:id="@+id/tvDate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.timestamp}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblDate"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerDate" />

                <TextView
                    android:id="@+id/tvMerchantId"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.merchantId}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblMerchantId"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerMerchantId" />

                <TextView
                    android:id="@+id/tvTerminalId"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.terminalId}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblTerminalId"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerTerminalId" />

                <TextView
                    android:id="@+id/tvBatchNumber"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.batchNumber}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblBatchNumber"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerBatchNum" />

                <TextView
                    android:id="@+id/tvInvoiceNumber"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.transNumber}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblInvoiceNumber"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerInvoice" />

                <TextView
                    android:id="@+id/tvCardNumber"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp"
                    android:text="@{txns.cardNumber}"
                    android:textSize="12sp"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblCardNumber"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerCardNumber" />

                <TextView
                    android:id="@+id/tvCardType"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.cardType}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblCardType"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerCardType" />

                <TextView
                    android:id="@+id/tvAuthNumber"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.authNumber}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblAuthNumber"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerDate" />

                <TextView
                    android:id="@+id/tvStatus"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp"
                    android:text="@{response.status}"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblStatus"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerDate" />

                <TextView
                    android:id="@+id/tvAuthNumber2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@{txns.currency + ' ' + txns.total}"
                    android:textAlignment="textEnd"
                    android:textStyle="bold"
                    app:layout_constraintBaseline_toBaselineOf="@+id/lblAuthNumber2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dividerDate" />

                <TextView
                    android:id="@+id/lblMerchantId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Merchant Id"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvDate" />

                <TextView
                    android:id="@+id/lblTerminalId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Terminal Id"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvMerchantId" />

                <TextView
                    android:id="@+id/lblBatchNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Batch Number"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvTerminalId" />

                <TextView
                    android:id="@+id/lblInvoiceNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Invoice Number"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvBatchNumber" />

                <TextView
                    android:id="@+id/lblCardNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Card Number"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/lblSale" />

                <TextView
                    android:id="@+id/lblCardType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Card Type"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvCardNumber" />

                <TextView
                    android:id="@+id/lblAuthNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="Auth Number"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvCardType" />

                <TextView
                    android:id="@+id/lblAuthNumber2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="60dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="AMOUNT:"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_details"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/lblDate"
                    app:layout_constraintTop_toBottomOf="@+id/tvAuthNumber" />

                <TextView
                    android:id="@+id/lblSale"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:fontFamily="@font/corsa_grotesk_regular"
                    android:text="@{txns.transType}"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_merchant_name"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/dividerInvoice"
                    app:layout_constraintStart_toStartOf="@+id/guideline8"
                    app:layout_constraintTop_toBottomOf="@+id/tvInvoiceNumber" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>


